<odoo>
  <data>
          <record id="account_move_line_view_inherit" model="ir.ui.view">
			<field name="name">account.move.form.view</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='invoice_line_ids']//tree//field[@name='price_unit']" position="before">
					<field name="roll" optional="hide"/>
                    <field name="pcs" optional="hide"/>
				</xpath>

			</field>
		</record>
          
         <record id="stock_move_line_view_inherit" model="ir.ui.view">
			<field name="name">stock.move.line.tree.view</field>
			<field name="model">stock.move.line</field>
			<field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='qty_done']" position="before">
				  <field name="total_weight" />
                                  <field name="weight"/>
				</xpath>

			</field>
		</record> 
     


      
  </data>
</odoo>