<odoo>
  <data>
   		<record id="account_move_line_view_inherit" model="ir.ui.view">
			<field name="name">account.move.form.view</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='invoice_line_ids']//tree//field[@name='price_unit']" position="before">
					<field name="roll" optional="hide"/>
                    <field name="pcs" optional="hide"/>
				</xpath>

			</field>
		</record>
  </data>
</odoo>