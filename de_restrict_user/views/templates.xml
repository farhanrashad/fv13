<odoo>
    <data>
       <template id="report_header_custom" inherit_id="web.internal_layout">
            <xpath expr="//div[@class='header']" position="replace">
                <div class ="header">                
                    <div style="margin-right:10px;padding:10px;left:10px;display:table-cell;">
                     
                   </div>
                </div>
            </xpath>
    </template> 
   <template id="report_header_external_custom" inherit_id="web.external_layout_standard">
            <xpath expr="//div[@class='header']" position="replace">
                <div class ="header">                
                    <div style="margin-right:10px;padding:10px;left:10px;display:table-cell;">
                     
                   </div>
                </div>
            </xpath>
   </template> 
   </data>
</odoo>