<odoo>
  <data>
      <record model="ir.ui.view" id="de_product_dimensions.product_template_form_view">
        <field name="name">de.product.dimensions.product.template.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='general_information']/group[1]" position="after">
                <group name="product_dimension" string=" Other Dimensions">
                    <group>
                        <field name="weight_sm"/>
                        <field name="width"/>
                        <field name="length"/>
                        <field name="height"/>
                        
                    </group>
                    <group>
                        <field name="dim_weight"/>
                    </group>
                </group>
            </xpath>
        </field>
    </record>
      
  </data>
</odoo>