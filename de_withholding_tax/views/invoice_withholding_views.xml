<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="view_move_form_inherit">
        <field name="name">view.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[1]" position="after">
                <page string="Withholding Tax" name="wht">
                    <field name="wht_line_ids" widget="one2many_list">
                        <tree string="WHT Lines" editable="bottom">
                            <field name="wht_type_id" />
                            <field name="base_amount" />
                            <field name="wht_amount" />
                        </tree>
                    </field>
                </page>
            </xpath>
            
        </field>
    </record>
</odoo>
